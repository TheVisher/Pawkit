#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules/@cloudworxx/tauri-plugin-mac-rounded-corners/scripts/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules/@cloudworxx/tauri-plugin-mac-rounded-corners/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules/@cloudworxx/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules/@cloudworxx/tauri-plugin-mac-rounded-corners/scripts/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules/@cloudworxx/tauri-plugin-mac-rounded-corners/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules/@cloudworxx/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/@cloudworxx+tauri-plugin-mac-rounded-corners@1.1.1_@tauri-apps+api@2.9.1/node_modules:/Users/minivish/Pawkit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cloudworxx/tauri-plugin-mac-rounded-corners/scripts/install.js" "$@"
else
  exec node  "$basedir/../@cloudworxx/tauri-plugin-mac-rounded-corners/scripts/install.js" "$@"
fi
