{
  "name": "test-bookmark-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:restart": "./scripts/restart-dev.sh",
    "build": "node scripts/vercel-build.js",
    "build:local": "node scripts/pre-migration-check.js && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "node scripts/pre-migration-check.js && prisma migrate dev",
    "prisma:migrate:deploy": "node scripts/pre-migration-check.js && prisma migrate deploy",
    "prisma:backup": "./scripts/backup-db.sh",
    "prisma:reset": "node scripts/protect-production-db.js reset && echo '⚠️  BLOCKED: Use prisma:reset:local for local DB only' && exit 1",
    "prisma:reset:local": "echo '⚠️  This will DELETE ALL local data!' && echo 'Run manually: npx prisma migrate reset' && exit 1",
    "prisma:push": "node scripts/pre-migration-check.js && prisma db push",
    "prisma:push:force": "node scripts/protect-production-db.js 'push --force-reset' && exit 1",
    "prisma:studio": "prisma studio",
    "db:backup": "npm run prisma:backup",
    "db:backup:production": "echo 'Use Supabase dashboard to create backups: https://supabase.com/dashboard' && exit 1",
    "test:e2e": "playwright test",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@mozilla/readability": "^0.6.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/jsdom": "^27.0.0",
    "@uiw/react-md-editor": "^4.0.8",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "idb": "^8.0.3",
    "isomorphic-dompurify": "^2.28.0",
    "jsdom": "^27.0.0",
    "lucide-react": "^0.544.0",
    "next": "15.5",
    "next-themes": "^0.3.0",
    "node-html-parser": "^7.0.1",
    "pdfjs-dist": "^5.4.394",
    "react": "^19.2.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-wiki-link": "^2.0.1",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.0",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0-canary.65",
    "postcss": "^8.4.41",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "@types/react-dom"
      ]
    }
  }
}
